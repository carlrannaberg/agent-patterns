{
  "name": "agent-patterns",
  "version": "1.0.0",
  "description": "AI agent patterns monorepo",
  "private": true,
  "scripts": {
    "issue": "bash scripts/create-issue.sh",
    "claude": "bash scripts/claude-context.sh",
    "dev": "pnpm --filter \"packages/*\" run dev",
    "build": "pnpm --filter \"packages/*\" run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "pnpm --filter \"packages/*\" run type-check",
    "prepare": "husky install"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@8.0.0"
}
